{
  "name": "mockettaro",
  "version": "1.1.7",
  "description": "Node Rest Mock Server",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R mocha-sonar-reporter"
  },
  "bin": {
    "mockettaro": "./commandline.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Marketto@github.com/Marketto/mockettaro.git"
  },
  "keywords": [
    "mock",
    "server",
    "rest",
    "json"
  ],
  "author": "Marco Ricupero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marketto/mockettaro/issues"
  },
  "homepage": "https://github.com/Marketto/mockettaro#readme",
  "config": {
    "mocha-sonar-reporter": {
      "testdir": "test",
      "classnameSuffix": ".js",
      "outputfile": "reports/test-report.xml"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "colors": "^1.2.1",
    "commander": "^2.15.1",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "jsonschema": "^1.2.2",
    "memory-cache": "^0.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.1.1",
    "mocha-sonar-reporter": "^0.1.6"
  }
}
